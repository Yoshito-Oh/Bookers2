  <main>
   <div class="container px-5 px-sm-0">
    <div class="row">
     <div class="col-sm-3">
      <%= render 'homes/part' %>
     </div>
     <div class="col-sm-8 offset-md-1">
      <h2>Users List</h2>
      <table class="table table-hover table-inverse">
       <thead>
        <tr>
         <th>image</th>
         <th>name</th>
         <th></th>
        </tr>
       </thead>
       <tbody>
         <% @users.each do |user| %>
           <tr>
            <td>
             <div class="icon-small"></div>
             
            </td>
            <td>
             <%= user.name %>
            </td>
            <td>
             <%= link_to "Show" , user_path(user.id)%>
            </td>
           </tr>
         <% end %>
        </tbody>
      </table>
    </div>
   </div>
  </main>